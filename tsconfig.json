{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "module": "es2020",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "metro-config.js",
    "assets/svg/index.js",
    "metro.config.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
